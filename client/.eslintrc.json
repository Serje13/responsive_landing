{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "es6": true,
            "modules": true
        }
    },
    "env": {
        "node": true
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "react": {
            "pragma": "React",
            "version": "16.4.0"
        }
    },
    "globals": {
        "document": true,
        "window": true,
        "Symbol": true,
        "fetch": true,
        "Headers": true,
        "Response": true,
        "URL": true,
        "Promise": true,
        "FileReader": true,
        "FormData": true,
        "Image": true,
        "File": true,
        "Buffer": true,
        "Blob": true,
        "Uint8Array": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "react/prop-types": "off",
        "react/jsx-uses-vars": "error",
        "react/jsx-indent": [
            "error",
            4
        ],
        "react/jsx-indent-props": [
            "error",
            4
        ],
        "curly": 2,
        "default-case": 1,
        "eqeqeq": [
            2,
            "smart"
        ],
        "global-require": 0,
        "handle-callback-err": 1,
        "no-console": 1,
        "no-debugger": 1,
        "no-obj-calls": 2,
        "no-alert": 0,
        "no-unreachable": 2,
        "no-caller": 2,
        "no-case-declarations": 0,
        "no-else-return": 0,
        "no-extend-native": 2,
        "no-extra-bind": 1,
        "no-extra-parens": 0,
        "no-extra-boolean-cast": 0,
        "no-magic-numbers": 0,
        "no-mixed-requires": 1,
        "no-multi-spaces": 2,
        "no-unused-expressions": 1,
        "no-unused-vars": 1,
        "no-1ing-comments": [
            0,
            {
                "terms": [
                    "TODO",
                    "FIXME",
                    "NOTE"
                ],
                "location": "anywhere"
            }
        ],
        "strict": [
            2,
            "global"
        ],
        "wrap-iife": [
            2,
            "outside"
        ],
        "no-var": 2,
        "no-catch-shadow": 2,
        "no-class-assign": 2,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "constructor-super": 2,
        "arrow-spacing": [
            2,
            {
                "before": true,
                "after": true
            }
        ],
        "template-curly-spacing": 2,
        "object-shorthand": [
            1,
            "always"
        ],
        "prefer-arrow-callback": 1,
        "prefer-spread": 1,
        "prefer-template": 1,
        "prefer-const": 1,
        "prefer-destructuring": 0,
        "prefer-rest-params": 1,
        "array-bracket-spacing": [
            1,
            "never"
        ],
        "block-spacing": [
            1,
            "never"
        ],
        "brace-style": [
            1,
            "1tbs",
            {
                "allowSingleLine": false
            }
        ],
        "camelcase": 0,
        "comma-spacing": [
            1,
            {
                "before": false,
                "after": true
            }
        ],
        "comma-style": [
            1,
            "last"
        ],
        "computed-property-spacing": [
            1,
            "never"
        ],
        "func-names": 0,
        "func-style": [
            1,
            "declaration",
            {
                "allowArrowFunctions": true
            }
        ],
        "linebreak-style": 0,
        "lines-around-comment": 0,
        "max-depth": [
            1,
            8
        ],
        "max-len": 0,
        "max-nested-callbacks": [
            1,
            8
        ],
        "max-params": [
            1,
            9
        ],
        "new-cap": 1,
        "new-parens": 1,
        "no-array-constructor": 1,
        "no-bitwise": 0,
        "no-continue": 0,
        "no-inline-comments": 0,
        "no-lonely-if": 1,
        "no-mixed-spaces-and-tabs": 2,
        "no-multiple-empty-lines": 1,
        "no-negated-condition": 0,
        "no-nested-ternary": 0,
        "no-new-object": 1,
        "no-plusplus": 0,
        "no-spaced-func": 1,
        "no-ternary": 0,
        "no-trailing-spaces": 1,
        "no-unneeded-ternary": 1,
        "object-curly-spacing": [
            1,
            "never"
        ],
        "one-var": 0,
        "operator-linebreak": [
            1,
            "after"
        ],
        "padded-blocks": [
            1,
            "never"
        ],
        "quote-props": [
            1,
            "consistent-as-needed"
        ],
        "quotes": [
            1,
            "double"
        ],
        "semi-spacing": [
            1,
            {
                "before": false,
                "after": true
            }
        ],
        "semi": [
            2,
            "always"
        ],
        "sort-vars": 0,
        "keyword-spacing": [
            1,
            {
                "before": true,
                "after": true
            }
        ],
        "space-before-blocks": [
            1,
            "always"
        ],
        "space-before-function-paren": [
            1,
            {
                "anonymous": "always",
                "named": "never"
            }
        ],
        "space-in-parens": [
            1,
            "never"
        ],
        "space-infix-ops": [
            1,
            {
                "int32Hint": true
            }
        ],
        "space-unary-ops": 2
    }
}